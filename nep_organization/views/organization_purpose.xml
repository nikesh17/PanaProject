<?xml version='1.0' encoding='utf-8'?>
<odoo>
   
    <record id="organization_purpose_view_form" model="ir.ui.view">
        <field name="name">organization_purpose_formation.view.form</field>
        <field name="model">organization.purpose</field>
        <field name="arch" type="xml">
            <form string=" Organization Purpose ">
                <sheet>
                    <group>
                        <field name="signed_document" widget="pdf_viewer" />
                        <field name="document_name" />
                        <field name="purpose_text"></field>
              
                    </group>
                </sheet>
            </form>
        </field>
    </record>


</odoo>