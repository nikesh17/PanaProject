<odoo>
    <data>
        <!-- Configuration Family Member Attributes Qualification Form View -->
        <record id="view_qualification_form" model="ir.ui.view">
            <field name="name">Qualification</field>
            <field name="model">farmer.qualification</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <!-- Configuration Family Member Attributes Occupation Form View -->
        <record id="view_occupation_form" model="ir.ui.view">
            <field name="name">Occupation</field>
            <field name="model">farmer.occupation</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <!-- Configuration Family Member Attributes relation Form View -->
        <record id="view_relation_form" model="ir.ui.view">
            <field name="name">relation</field>
            <field name="model">farmer.family.relation</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        
    </data>
</odoo>