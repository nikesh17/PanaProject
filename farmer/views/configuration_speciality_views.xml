<odoo>
    <data>
        <!-- Configuration Expert Speciality Form View -->
        <record id="view_expert_speciality_form" model="ir.ui.view">
            <field name="name">Expert Speciality Form</field>
            <field name="model">expert.type</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name"/>
                            
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        
        <!-- Configuration Expert Speciality Tree View -->
        <record id="view_epert_speciality_tree" model="ir.ui.view">
            <field name="name">Expert Speciality Tree</field>
            <field name="model">expert.type</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                </tree>
            </field>
        </record>
        
    </data>
</odoo>